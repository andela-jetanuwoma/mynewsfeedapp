{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "js-cookie": "^2.1.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "node-sass": "^4.5.2",
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "build-css": "node-sass src/assets/style -o src/assets/style",
    "watch-css": "npm run build-css && node-sass src/assets/style -o src/ --watch --recursive",
