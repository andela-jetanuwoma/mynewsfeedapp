    "react-scripts": "0.9.5",
    "semantic-ui": "^2.2.10"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/style -o src/assets/style",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/style -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls"
  }
